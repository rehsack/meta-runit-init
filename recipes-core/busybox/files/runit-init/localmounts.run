#!/bin/sh

. /etc/default/rcS
. /etc/init.d/functions-ri

serviceran ../1-mdev || serviceup ../udev || exit 1

if test -f /forcefsck; then
	force="-f"
else
	force=""
fi

if test "$FSCKFIX" = "yes"; then
	fix="-y"
else
	fix="-a"
fi

/sbin/fsck -AR $force $fix
mount -at nonfs,nosmbfs,noncpfs 2>/dev/null

markran

exit 0
